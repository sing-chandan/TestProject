</head>

<style>
    .bg-col-Graph-admin {
        min-width: 95%;
        margin-bottom: 15px;
        /*background-color: rgba(47, 49, 53, 0.05);*/
        background-image: url(@AppBaseURL/Content/images/sand.png);
        min-height: 350px;
        height: 100%;
    }
    .field-validation-error {
        color: #e80c4d;
        list-style-type: none;
    }

    .field-validation-valid {
        display: none;
        list-style-type: none;
    }

    input.input-validation-error {
        border: 1px solid #e80c4d;
        list-style-type: none;
    }

    input[type="checkbox"].input-validation-error {
        border: 0 none;
        list-style-type: none;
    }

    .validation-summary-errors {
        color: #e80c4d;
        list-style-type: none;
        font-size: 1.1em;
    }

    .validation-summary-valid {
        display: none;
        list-style-type: none;
    }

    ul {
        list-style-type: none;
    }

    .h2txt {
        color: #152f48;
    }
</style>

<body>
    <div class="container">
        <div class="row">
            <div class="col-sm-4 img-responsive logo-aling" style="padding-bottom:1%;padding-top:1%;"><img src="@AppBaseURL/content/images/rgl_logo.png" class="logo-image" /></div>
            <div class="col-sm-8">
                <div class="col-sm-12"><div style="text-align: right; padding-top: 1%; color: #4C638E;">@Html.Partial("_LoginPartial")</div></div>
                <div class="col-sm-12" style="padding-bottom:1%;padding-top:1%;">
                </div>
            </div>
        </div>
        <div class="">
            <a class="toggleMenu glyphicon glyphicon-align-justify " href="#" style="color:white;"></a>
            <ul class="nav">
                <li class="test">@Html.ActionLink("Dashboard", "Index", "Admin", null, new { @class = "a" })</li>
                <li class="test">@Html.ActionLink("Users", "Index", "CreateUser", null, new { @class = "a" })</li>
                <li class="test">@Html.ActionLink("Configuration", "Index", "Configuration", null, new { @class = "a" })</li>
                <li class="test">@Html.ActionLink("Customers", "CustomerList", "Customer", null, new { @class = "a" })</li>
                <li class="test">@Html.ActionLink("Subscribers", "SubscriberList", "Subscriber", null, new { @class = "a" })</li>
                <li>@Html.ActionLink("Change Password", "Manage", "Account", null, new { @class = "a" })</li>
                @*<li><a href="javascript:document.getElementById('logoutForm').submit()">Log off</a></li>0*@
            </ul>
        </div>

        <hr />
        <div class="row">
            @RenderBody()
        </div>
        <div class="row navbar-fixed-bottom " style="background-color: rgb(47, 49, 53);">
