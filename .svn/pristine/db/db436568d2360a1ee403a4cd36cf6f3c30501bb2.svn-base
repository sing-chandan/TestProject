.tooltips {
    display: inline;
    position: relative;
    text-decoration: none;
    top: 0px;
    left: 4px;
}

    .tooltips:hover:after {
        background: #333;
        background: rgba(0,0,0,.8);
        border-radius: 5px;
        top: -5px;
        color: #fff;
        content: attr(alt);
        left: 180px;
        padding: 5px 15px;
        position: absolute;
        z-index: 98;
        width: 200px;
    }

    .tooltips:hover:before {
        border: solid;
        border-color: transparent black;
        border-width: 6px 6px 6px 0;
        bottom: 20px;
        content: "";
        left: 175px;
        position: absolute;
        z-index: 99;
        top: 3px;
    }

